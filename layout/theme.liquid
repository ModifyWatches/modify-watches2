      ///////// pop up promo //////////
      if ( (!$.cookie('summersale')) ) {
     //   $('#bigPopup').delay(300).fadeIn(200);
      } else {
        $('#promoBar').slideDown(0,function(){
          $('.site-header').addClass('hasPromo');
